# Server Configuration
PORT=5000
NODE_ENV=development

# Blockchain RPC URLs (Primary)
MAINNET_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
BASE_RPC_URL=https://mainnet.base.org
POLYGON_RPC_URL=https://polygon-rpc.com
GOERLI_RPC_URL=https://eth-goerli.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
LINEA_RPC_URL=https://rpc.linea.build
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
OPTIMISM_RPC_URL=https://mainnet.optimism.io

# Alternative RPC URLs (Fallback)
MAINNET_RPC_URL2=https://eth-mainnet.public.blastapi.io
BASE_RPC_URL2=https://base.blockpi.network/v1/rpc/public
POLYGON_RPC_URL2=https://polygon.llamarpc.com
GOERLI_RPC_URL2=https://eth-goerli.public.blastapi.io
LINEA_RPC_URL2=https://linea.drpc.org
ARBITRUM_RPC_URL2=https://arbitrum-one.public.blastapi.io
OPTIMISM_RPC_URL2=https://optimism.public.blastapi.io

# Contract Addresses (Deploy contracts and get the address)
MAINNET_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
BASE_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
POLYGON_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
GOERLI_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
LINEA_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
ARBITRUM_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
OPTIMISM_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000

# Private Keys for Testing (KEEP SECURE!)
PRIVATE_KEY=your_private_key_here
PRIVATE_KEY1=your_private_key_1_here
PRIVATE_KEY2=your_private_key_2_here
PRIVATE_KEY3=your_private_key_3_here

# Client URL for CORS
CLIENT_URL=http://localhost:3000
REDIS_URL=redis://localhost:6379
INTERNAL_API_TOKEN=change_me
ENABLE_INTERNAL_CRON=false

# API Keys (Optional - for enhanced token detection)
ETHERSCAN_API_KEY=your_etherscan_api_key
LINEASCAN_APIKEY_V2=your_lineascan_v2_api_key  # For Linea contract verification (Etherscan v2)
COVALENT_API_KEY=your_covalent_api_key
MORALIS_API_KEY=your_moralis_api_key
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key

# Gas Reporting
REPORT_GAS=true

# Database Configuration (for production)
DATABASE_URL=your_database_url_here

# Security
JWT_SECRET=your_jwt_secret_here
SESSION_SECRET=your_session_secret_here

# Monitoring
SENTRY_DSN=your_sentry_dsn_here

# Extra token lists per network (comma-separated addresses)
# Example to ensure detection of tokens like meUSDT, STG on Linea:
# LINEA_EXTRA_TOKENS=0xTokenAddress1,0xTokenAddress2
LINEA_EXTRA_TOKENS=
MAINNET_EXTRA_TOKENS=
BASE_EXTRA_TOKENS=
POLYGON_EXTRA_TOKENS=
ARBITRUM_EXTRA_TOKENS=
OPTIMISM_EXTRA_TOKENS=
GOERLI_EXTRA_TOKENS=

# Multicall Contract Addresses (for efficient batch token queries)
# These are the official Multicall contract addresses for each network
LINEA_MULTICALL_ADDRESS=0x5BA1e12693Dc8F9c48aAD8770482f4739bEeD696
ARBITRUM_MULTICALL_ADDRESS=0x5BA1e12693Dc8F9c48aAD8770482f4739bEeD696
